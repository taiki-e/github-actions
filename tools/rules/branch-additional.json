{
  "name": "Additional branch protection for non-admin roles",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": [
        "~DEFAULT_BRANCH",
        "refs/heads/v[0-9].[0-9]",
        "refs/heads/v[0-9].[1-9][0-9]",
        "refs/heads/v[0-9].[1-9][0-9][0-9]",
        "refs/heads/v[1-9]",
        "refs/heads/v[1-9][0-9]",
        "refs/heads/v[1-9][0-9][0-9]",
        "refs/heads/v[1-9][0-9].[0-9]",
        "refs/heads/v[1-9][0-9].[1-9][0-9]",
        "refs/heads/v[1-9][0-9].[1-9][0-9][0-9]",
        "refs/heads/v[1-9][0-9][0-9].[0-9]",
        "refs/heads/v[1-9][0-9][0-9].[1-9][0-9]",
        "refs/heads/v[1-9][0-9][0-9].[1-9][0-9][0-9]",
        "refs/heads/releases/v[0-9].[0-9]",
        "refs/heads/releases/v[0-9].[1-9][0-9]",
        "refs/heads/releases/v[0-9].[1-9][0-9][0-9]",
        "refs/heads/releases/v[1-9]",
        "refs/heads/releases/v[1-9][0-9]",
        "refs/heads/releases/v[1-9][0-9][0-9]",
        "refs/heads/releases/v[1-9][0-9].[0-9]",
        "refs/heads/releases/v[1-9][0-9].[1-9][0-9]",
        "refs/heads/releases/v[1-9][0-9].[1-9][0-9][0-9]",
        "refs/heads/releases/v[1-9][0-9][0-9].[0-9]",
        "refs/heads/releases/v[1-9][0-9][0-9].[1-9][0-9]",
        "refs/heads/releases/v[1-9][0-9][0-9].[1-9][0-9][0-9]"
      ]
    }
  },
  "rules": [
    {
      "type": "pull_request",
      "parameters": {
        "required_approving_review_count": 0,
        "dismiss_stale_reviews_on_push": false,
        "required_reviewers": [],
        "require_code_owner_review": false,
        "require_last_push_approval": false,
        "required_review_thread_resolution": false,
        "allowed_merge_methods": ["squash", "rebase"]
      }
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "required_status_checks",
      "parameters": {
        "strict_required_status_checks_policy": false,
        "do_not_enforce_on_create": false,
        "required_status_checks": [
          {
            "context": "tidy / prepare",
            "integration_id": 15368
          },
          {
            "context": "tidy / tidy",
            "integration_id": 15368
          }
        ]
      }
    }
  ],
  "bypass_actors": [
    {
      "actor_id": 5,
      "actor_type": "RepositoryRole",
      "bypass_mode": "always"
    }
  ]
}
